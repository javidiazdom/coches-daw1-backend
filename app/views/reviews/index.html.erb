<%= link_to 'New article', new_review_path %>
<table>
    <% @reviews.each do |review| %>
        <tr>
            <td><%= review.title %></td>
            <td><%= link_to  "Show", review_path(review) %></td>
            <td><%= link_to  "Destroy", review_path(review), method: :delete %></td>
        </tr>
    <%end%>
</table>