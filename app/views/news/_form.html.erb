<%= form_for @new do |f| %>

    <%= f.label :title %> <br>
    <%= f.text_field :title %> <br>

    <%= f.label :description %> <br>
    <%= f.text_field :description %> <br>

    <%= f.label :picture_url %> <br>
    <%= f.text_field :picture_url %> <br>

    <%= f.label :category %> <br>
    <%= f.text_field :category %> <br>

    <%= f.submit %>


<% end %>